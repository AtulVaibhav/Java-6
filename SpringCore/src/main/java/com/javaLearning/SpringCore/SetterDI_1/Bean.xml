<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean id="course" class="com.javaLearning.SpringCore.SetterDI_1.Course">
      <property name="courseName" value="Java"/>
    </bean>
    <bean id="student" class="com.javaLearning.SpringCore.SetterDI_1.Student">
      <property name="id" value="101"/>
      <property name="name" value="Aayush"/>
      <property name="course" ref="course"/>
    </bean>
    
</beans>